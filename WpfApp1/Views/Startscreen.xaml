<Window x:Class="WpfApp1.Startscreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="startscreen" Height="500" Width="600
">
    <Grid Background="#FF00175D">
        <Button x:Name="btncinemaprogram" Content="Biografprogram" HorizontalAlignment="Center" Margin="0,201,0,0" VerticalAlignment="Top" Height="42" Width="204" Background="#FF908F8F" Click="btncinemaprogram_Click"/>
        <Button Content="Scenarie 3 - billetter" HorizontalAlignment="Center" Margin="0,269,0,0" VerticalAlignment="Top" Height="42" Width="204" Background="#FF8A8989"/>
        <Button x:Name="btnaddmovie" Content="Tilføj film" HorizontalAlignment="Center" Margin="0,127,0,0" VerticalAlignment="Top" Height="42" Width="204" Click="Button_Click" Background="#FF949292" />
        <Label Content="Movie Jenses program" HorizontalAlignment="Center" Margin="0,19,0,0" VerticalAlignment="Top" FontSize="50"/>

    </Grid>
</Window>
